{
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "postcss-cli": "^9.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "browsersync": "^0.0.1-security",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24"
  },
  "scripts": {
    "start": "npm-run-all --parallel css eleventy",
    "css": "postcss src/css/tailwind.css --o dist/css/tw.css --watch",
    "eleventy": "eleventy --serve",
    "browsersync": "browser-sync start --server 'dist' --files 'dist' --port 8080 --no-notify --no-open"

  }
}
